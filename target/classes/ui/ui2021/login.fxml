<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controller.LoginController">
   <children>
      <Button fx:id="button" layoutX="124.0" layoutY="144.0" mnemonicParsing="false" onAction="#userLogIn" prefHeight="25.0" prefWidth="52.0" text="Login" />
      <Label fx:id="lbUsername" layoutX="14.0" layoutY="41.0" text="Username:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="lbPassword" layoutX="16.0" layoutY="82.0" text="Password:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="100.0" layoutY="39.0" promptText="Usernane" />
      <PasswordField fx:id="password" layoutX="100.0" layoutY="79.0" promptText="Password" />
      <Label fx:id="wrongLogIn" alignment="CENTER" contentDisplay="CENTER" layoutX="23.0" layoutY="119.0" prefHeight="17.0" prefWidth="254.0" textFill="RED" />
   </children>
</AnchorPane>
